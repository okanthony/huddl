<h1>Game Name?</h1>

<h3>Game Info</h3>
<ul>
  <li><%= @game.game_day.strftime('%b %eth, %Y') %> <%= @game.game_time.in_time_zone('EST').strftime('%l:%M %p') %></li>
  <li><%= @game.street %> <%= @game.city %>, <%= @game.state %> <%= @game.zip %></li>
</ul>

<h3>Roster</h3>
<ul>
<% if @roster.empty? %>
  <% 9.times do %>
    <li>Unclaimed - Button</li>
  <% end %>
<% else %>
  <% @roster.each do |roster| %>
    <li><%= roster.user %></li>
  <% end %>
<% end %>
</ul>
